ode...!\");\n      } else {\n        axios\n          .post(\"http://localhost:4001/v1/api/manager\", formData)\n          .then((res) => {\n            console.log(res.data);\n            history.replace(\"/\");\n          })\n          .catch((err) => {\n            console.lof(err);\n          });\n      }\n    }\n  };\n\n  return (\n    <div style={{ maxWidth: 500, margin: \"auto\" }}>\n      <div className=\"mb-3\">\n        <input\n          className=\"form-control\"\n          type=\"text\"\n          name=\"Manager Name\"\n          placeholder=\"Manager Name\"\n          onChange={(e) => setManagerName(e.target.value)}\n        />\n      </div>\n      <div className=\"mb-3\">\n        <input\n          className=\"form-control\"\n          type=\"text\"\n          name=\"Bank Name\"\n          placeholder=\"Bank Name\"\n          onChange={(e) => setBankName(e.target.value)}\n        />\n      </div>\n      <div className=\"mb-3\">\n        <input\n          className=\"form-control\"\n          type=\"date\"\n          name=\"Bank Gurantee Code\"\n          onChange={(e) => setBankGuranteeCode(e.target.value)}\n        />\n      </div>\n      <div className=\"mb-3\">\n        <select\n          className=\"form-select\"\n          aria-label=\"Select Company\"\n          onChange={(e) => setCompany(e.target.value)}\n        >\n          <option selected>Select Company</option>\n          <option value=\"Abans Finance\">Abans Finance</option>\n          <option value=\"Sampath Bank PLC\">Sampath Bank PLC</option>\n          <option value=\"IFC\">IFC</option>\n          <option value=\"Lanka Business Loan\">Lanka Business Loan</option>\n          <option value=\"Asian Development Bank\">Asian Development Bank</option>\n        </select>\n      </div>\n      <div className=\"mb-3\">\n        <input\n          className=\"form-control\"\n          type=\"text\"\n          name=\"Bank Gurantee Code\"\n          placeholder=\"Bank Gurantee Code\"\n          onChange={(e) => setBankGuranteeCode(e.target.value)}\n        />\n      </div>\n      <div className=\"mb-3\">\n        <input\n          className=\"form-control\"\n          type=\"text\"\n          name=\"Bank Gurantee Amount\"\n          placeholder=\"Bank Gurantee Amount\"\n          onChange={(e) => setBankGuranteeAmount(e.target.value)}\n        />\n      </div>\n      <div className=\"mb-3\">\n        <input\n          className=\"form-control\"\n          type=\"email\"\n          name=\"Email\"\n          placeholder=\"Email\"\n          onChange={(e) => setEmail(e.target.value)}\n        />\n      </div>\n      <div className=\"text-center\">\n        <button className=\"btn btn-primary\" onClick={handleSubmit}>\n          Submit\n        </button>\n      </div>\n    </div>\n  );\n};\n\nexport default AddManager;\n",{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":8,"column":34,"nodeType":"37","messageId":"38","endLine":8,"endColumn":59},"no-native-reassign",["39"],"no-negated-in-lhs",["40"],"no-unused-vars","'setBankGuranteeExpireDate' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]